<!-- @group.disable -->
<templateSet group="EditorGroups">
    <template name="@gc"
              value="@group.color $CARET$"
              toReformat="true"
              toShortenFQNames="true"
              key="live.template.@.gc.description"
              resource-bundle="messages.EditorGroupsBundle">
        <variable name="CARET" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="OTHER" value="true"/>
        </context>
    </template>
    <template name="@gr"
              value="@group.related $CARET$"
              toReformat="true"
              toShortenFQNames="true"
              key="live.template.@.gr.description"
              resource-bundle="messages.EditorGroupsBundle">
        <variable name="CARET" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="OTHER" value="true"/>
        </context>
    </template>
    <template name="@gt"
              value="@group.title $CARET$"
              toReformat="true"
              toShortenFQNames="true"
              key="live.template.@.gt.description"
              resource-bundle="messages.EditorGroupsBundle">
        <variable name="CARET" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="OTHER" value="true"/>
        </context>
    </template>
</templateSet>
